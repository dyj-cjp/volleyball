<!--pages/play/index.wxml-->
<view class="nav">
  <view class="title">我发起的</view>
  <view class="nav_item">
    <view class="nav_item1" wx:for="{{dataList}}" wx:key="index" bindtap="itemclick" data-title="{{item.title}}">
      <view class="indexid">{{index+1}}</view>
      <view class="playdetail">
        <view class="playtitle">{{item.title}}</view>
        <view class="playdata">{{item.time}}</view>
      </view>
      <view class="number">
        <view class="playch">现/总</view>
        <view class="playnumber">{{item.selectednum}}/{{item.selectnum}}人</view>
      </view>
    </view>
  </view>
</view>